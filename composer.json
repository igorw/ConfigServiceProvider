{
    "name": "igorw/config-service-provider",
    "description": "A config ServiceProvider for Silex with support for php, json and yaml.",
    "keywords": ["silex"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/igorw/ConfigServiceProvider/contributors"
        }
    ],
    "require": {
        "silex/silex": "2.0.x-dev"
    },
    "require-dev": {
        "symfony/yaml": "~2.1",
        "jamesmoss/toml": "~0.1"
    },
    "suggest": {
        "symfony/yaml": "~2.1",
        "jamesmoss/toml": "~0.1"
    },
    "autoload": {
        "psr-4": { "Igorw\\Silex\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
